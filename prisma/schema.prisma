generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id            String    @id @default(cuid())
  firebaseUid   String    @default(cuid())
  name          String
  surname       String
  email         String?   @unique
  emailVerified DateTime?
  birthDate     DateTime
  image         String?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}
